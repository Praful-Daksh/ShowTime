{
    "version": 2,
    "builds": [
      { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/", "dest": "/app.js" },
      { "src": "/login", "dest": "/app.js" },
      { "src": "/dashboard", "dest": "/app.js" },
      { "src": "/(.*)", "dest": "/app.js" }
    ]
  }
  